  "StatThreshold": {
    "config": {
      "default": {}
    },
    "post_rule_train_config": {
      "default": {"metric": null, "unsup_quantile": 0.995}
    }
  },

  "SR": {"alias": "SpectralResidual"},
  "SpectralResidual": {
    "config": {
      "default": {},
