        "damped_trend": true,
        "transform": {"name": "TemporalResample", "granularity": "1h"}
      },
      "IOpsCompetition": {
        "transform": {"name": "TemporalResample", "granularity": "5min"}
      },
      "CASP": {
        "transform": {"name": "TemporalResample", "granularity": "5min"}
      }
    }
  },

  "Prophet": {"alias": "ProphetDetector"},
